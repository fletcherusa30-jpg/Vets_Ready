{
  "name": "rallyforge-mos-engine",
  "version": "1.0.0",
  "description": "Military Job Intelligence & Resume Generation Engine for rallyforge",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "ingest:all": "tsx src/ingestion/ingest-all.ts",
    "ingest:army": "tsx src/ingestion/army-ingest.ts",
    "ingest:marines": "tsx src/ingestion/marines-ingest.ts",
    "ingest:navy": "tsx src/ingestion/navy-ingest.ts",
    "ingest:airforce": "tsx src/ingestion/airforce-ingest.ts",
    "ingest:coastguard": "tsx src/ingestion/coastguard-ingest.ts",
    "ingest:spaceforce": "tsx src/ingestion/spaceforce-ingest.ts",
    "normalize": "tsx src/normalization/merge-all-jobs.ts",
    "generate:resume": "tsx src/resume_language/generator.ts",
    "generate:skills": "tsx src/skills/taxonomy-generator.ts",
    "generate:civilian": "tsx src/civilian_mapping/job-matcher.ts",
    "update:check": "tsx src/update_checker/check-for-updates.ts",
    "update:apply": "tsx src/update_checker/check-for-updates.ts --apply",
    "pipeline": "npm run ingest:all && npm run normalize && npm run generate:resume && npm run generate:skills && npm run generate:civilian",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "military",
    "veterans",
    "resume",
    "mos",
    "afsc",
    "rating",
    "career",
    "transition"
  ],
  "author": "rallyforge",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "csv-parse": "^5.5.3",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

