{
  "name": "vetsready-platform",
  "version": "0.1.0",
  "description": "VetsReady: One-Stop Veteran Support Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest 2>&1 || echo 'Tests skipped'",
    "test:watch": "jest --watch 2>&1 || echo 'Watch tests skipped'",
    "lint": "eslint . --ext .ts --ignore-path .gitignore 2>&1 || true",
    "format": "prettier --write \"**/*.ts\" || true",
    "start": "node dist/index.js",
    "validate": "tsc --noEmit"
  },
  "keywords": [
    "veterans",
    "benefits",
    "employment",
    "education",
    "wellness"
  ],
  "author": "VetsReady Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.40.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "prettier": "^2.8.8"
  }
}
